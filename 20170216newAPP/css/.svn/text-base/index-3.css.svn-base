#sw{
	width:100%;
	height:300px;
}
.s1,.s2,.s3,.s4{
	width:100%;
	height:100%;
	font-family: 'Microsoft Yahei',Tahoma,Helvetica,Arial,sans-serif;
}
.s1{
	background:url(../images/banner1.png) no-repeat 0 0;
	background-size:100% 100%;
	position:relative;
}
.s2{
	background:url(../images/banner2.png) no-repeat 0 0;
	background-size:100% 100%;
	position:relative;
}
.s3{
	background:url(../images/banner3.png) no-repeat 0 0;
	background-size:100% 100%;
	position:relative;
}
.s4{
	background:url(../images/banner4.png) no-repeat 0 0;
	background-size:100% 100%;
	position:relative;
}
/*以下是主要内容*/
.home-main{
	width:100%;
	height:600px;
	box-sizing: border-box;
	padding:10px;	
}
.main-left{
	width:50%;
	height:580px;
	float:left;
}
.main-right{
	width:50%;
	height:580px;
	float:left;
}
.home-menu{
	margin:5px;
	width:300px;
	height:188px;
	background:#fff;
	text-align: center;
	box-shadow: 2px 2px 2px 2px #ccc;
	border-radius: 10px;
}
.home-menu dt{
	height:188px;
	width:150px;
	box-sizing: border-box;
	float:left;
	padding-top:14px;
	padding-left:5px;
}
.home-menu dd{
	float:left;
	width:150px;
	height:188px;
	line-height:188px;
	font-size:30px;
	text-align: center;
}
.home-menu-diff{
	margin:5px;
	width:300px;
	height:188px;
	background:#fff;
	box-shadow: 2px 2px 2px 2px #ccc;
	border-radius: 10px;
}
.home-menu-diff dt{
	float:left;
	width:150px;
	text-align: center;
	box-sizing: border-box;
	padding-top:14px;
	padding-left:5px;
}
.home-menu-diff dd{
	float:left;
	width:150px;
	height:188px;
	line-height:188px;
	font-size:30px;
	text-align: center;
}
