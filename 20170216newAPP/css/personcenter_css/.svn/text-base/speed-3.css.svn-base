body,html{
	width: 100%;
	height: 100%;
}
body{
	background: #f7f7f7;
}
.coach-list-cont{
	width: 100%;
	height: 100%;
	overflow: hidden;
	position:relative;
}
#list-box{
	height:auto;
	background: #fff;	
}
.speed-detail{
	width: 100%;
	min-height: 400px;
	height: auto;
	background: #fff;
	margin-top: 100px;
	padding: 10px 0;
	box-sizing: border-box;
}
.speedNone{
	width: 300px;
	height: 80px;
	line-height: 80px;
	font-size: 34px;
	margin: 0 auto;	
	display: none;
}
.speedMsg{
	width: 100%;
	height:auto;
	overflow: hidden;
	margin-top: 10px;	
}
.speedList{
	width: 100%;
	height: 100px;
	padding:0 10px;
	font-size: 30px;
	line-height: 100px;
	box-sizing: border-box;
	border-bottom: 1px solid #ccc;
}
.speedList p:nth-child(1){
	width: 40%;
	float: left;
}
.speedList p:nth-child(2){
	width: 60%;
	float: left;
	padding-left: 50px;
	box-sizing: border-box;
}
.speedList p span{
	padding-left: 12px;
}
.speedList p:nth-child(2) span{
	color: #F5A521;
}
